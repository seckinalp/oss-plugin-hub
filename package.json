{
  "name": "oss-plugin-hub",
  "version": "0.1.0",
  "type": "module",
  "description": "Plugin Discovery Hub - Aggregating open-source plugins for Obsidian and beyond",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:fast": "node scripts/optimize-dev.js && next dev",
    "dev:optimize": "node scripts/optimize-dev.js",
    "build": "node scripts/optimize-build.js && next build",
    "build:analyze": "ANALYZE=true next build",
    "build:fast": "node scripts/optimize-build.js && next build --no-lint",
    "build:optimize": "node scripts/optimize-build.js",
    "start": "next start",
    "lint": "next lint",
    "fetch-obsidian": "node scripts/fetch-obsidian-plugins.js",
    "fetch-vscode": "node scripts/fetch-vscode-plugins.js",
    "fetch-firefox": "node scripts/fetch-firefox-addons.js",
    "fetch-chrome": "node scripts/fetch-chrome-extensions.js",
    "fetch-opera": "node scripts/fetch-opera-extensions.js",
    "fetch-homeassistant": "node scripts/fetch-homeassistant-components.js",
    "fetch-wordpress": "node scripts/fetch-wordpress-plugins.js",
    "fetch-jetbrains": "node scripts/fetch-jetbrains-plugins.js",
    "fetch-sublime": "node scripts/fetch-sublime-plugins.js",
    "fetch-minecraft-modrinth": "node scripts/fetch-modrinth-mods.js",
    "fetch-minecraft-curseforge": "node scripts/fetch-curseforge-mods.js",
    "fetch-minecraft-spiget": "node scripts/fetch-spiget-plugins.js",
    "fetch-minecraft": "npm run fetch-minecraft-modrinth && npm run fetch-minecraft-spiget",
    "deduplicate-minecraft": "node scripts/deduplicate-minecraft-plugins.js",
    "fetch-all-sources": "node scripts/fetch-obsidian-plugins.js && node scripts/fetch-vscode-plugins.js && node scripts/fetch-firefox-addons.js && node scripts/fetch-homeassistant-components.js && node scripts/fetch-wordpress-plugins.js && node scripts/fetch-jetbrains-plugins.js && node scripts/fetch-sublime-plugins.js && npm run fetch-minecraft",
    "fetch-download-stats": "node scripts/fetch-download-stats.js",
    "fetch-download-stats:obsidian": "node scripts/fetch-download-stats.js obsidian",
    "update-stats": "node scripts/update-github-stats.js",
    "update-stats:force": "node scripts/update-github-stats.js --force",
    "update-stats:obsidian": "node scripts/update-github-stats.js --platform obsidian",
    "update-stats:vscode": "node scripts/update-github-stats.js --platform vscode",
    "update-stats:firefox": "node scripts/update-github-stats.js --platform firefox",
    "update-stats:homeassistant": "node scripts/update-github-stats.js --platform homeassistant",
    "update-stats:obsidian:force": "node scripts/update-github-stats.js --platform obsidian --force",
    "update-stats:vscode:force": "node scripts/update-github-stats.js --platform vscode --force",
    "update-stats:firefox:force": "node scripts/update-github-stats.js --platform firefox --force",
    "update-stats:wordpress": "node scripts/update-github-stats.js --platform wordpress",
    "update-stats:wordpress:force": "node scripts/update-github-stats.js --platform wordpress --force",
    "update-stats:jetbrains": "node scripts/update-github-stats.js --platform jetbrains",
    "update-stats:jetbrains:force": "node scripts/update-github-stats.js --platform jetbrains --force",
    "update-stats:sublime": "node scripts/update-github-stats.js --platform sublime",
    "update-stats:sublime:force": "node scripts/update-github-stats.js --platform sublime --force",
    "update-stats:minecraft": "node scripts/update-github-stats.js --platform minecraft",
    "update-stats:minecraft:force": "node scripts/update-github-stats.js --platform minecraft --force",
    "update-stats:chrome": "node scripts/update-github-stats.js --platform chrome",
    "update-stats:chrome:force": "node scripts/update-github-stats.js --platform chrome --force",
    "fetch-all": "npm run fetch-all-sources && npm run update-stats",
    "generate-analytics": "node scripts/generate-analytics-summary.js",
    "generate-github-analytics": "node scripts/generate-github-analytics.js",
    "analyze-repo-duplicates": "node scripts/analyze-repo-duplicates.js",
    "fetch-sbom": "node scripts/fetch-sbom.js",
    "fetch-missing-sboms": "node scripts/fetch-missing-sboms.js",
    "fetch-openssf-scorecard": "node scripts/fetch-openssf-scorecard.js",
    "fetch-codeql": "node scripts/fetch-codeql-analysis.js",
    "scan-osv": "node scripts/scan-osv-from-sbom.js",
    "analyze-vulnerabilities": "node scripts/analyze-osv-vulnerabilities.js",
    "view-vulnerability-report": "node scripts/view-vulnerability-report.js",
    "vulnerability-summary": "node scripts/generate-vulnerability-summary.js",
    "enrich-vulnerability-data": "node scripts/enrich-with-vulnerability-data.js"
  },
  "dependencies": {
    "dotenv": "17.2.3",
    "next": "^14.2.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-markdown": "^9.0.0",
    "remark-gfm": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.4.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
